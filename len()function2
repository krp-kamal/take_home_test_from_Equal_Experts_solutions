str1 = '123'
str2 = '4567'

int1 = int(str1)
int2 = int(str2)

sum_of_values = int1 + int2

sum_of_lengths = len(str1) + len(str2)

print(f"The sum of the integer values of '{str1}' and '{str2}' is: {sum_of_values}")
print(f"The sum of the lengths of '{str1}' and '{str2}' is: {sum_of_lengths}")

