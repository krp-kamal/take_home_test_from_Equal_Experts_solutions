# Integer to String#
num = 30
num_str = str(num)
print(num_str)

# String to Integer
num_str = '10'
num = int(num_str)
print(num)

# String to Character
char_str = 'A'
char = char_str[0]
print(char)

# Character to String
char = 'A'
char_str = str(char)
print(char_str) 

# Integer to Boolean
num = 1
bool_val = bool(num)
print(bool_val)  

# Boolean to Integer
bool_val = True
num = int(bool_val)
print(num) 

# Boolean to String
bool_val = True
str_val = str(bool_val)
print(str_val) 

# Float to Integer
flt = 10.56
num = int(flt)
print(num)

# Integer to Float
num = 10
flt = float(num)
print(flt)  

